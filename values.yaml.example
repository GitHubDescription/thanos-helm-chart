prometheus-operator:
  prometheus:
    prometheusSpec:
      replicas: 2
      externalLabels:
        region: local
      thanos:
        objectStorageConfig:
          # this should be ${releaseName}-thanos-object-store
          name: thanos-thanos-object-store
          key: store.yaml

query:
  replicaCount: 2
  logLevel: info
  queryReplicaLabel: replica

store:
  replicaCount: 2
